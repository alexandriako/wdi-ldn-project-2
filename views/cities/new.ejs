<form action="/cities" method="post">
  <div class="form-group">
    <label for="name">Name</label>
    <input class="form-control" type="text" name="name">
  </div>

  <div class="form-group">
    <label for="image">Image</label>
    <input class="form-control" type="text" name="image">
  </div>

  <div class="form-group">
    <label for="state">State</label>
    <input class="form-control" type="text" name="state">
  </div>

  <div class="form-group">
    <label for="dressCode">Dress code</label>
    <input class="form-control" type="text" name="dressCode">
  </div>

  <div class="form-group">
    <label for="lat">Latitude</label>
    <input class="form-control" type="text" name="lat">
  </div>

  <div class="form-group">
    <label for="lng">Longitude</label>
    <input class="form-control" type="text" name="lng">
  </div>

  <button class="btn btn-success">Create City!</button>
</form>
