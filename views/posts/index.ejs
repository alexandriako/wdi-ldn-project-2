<div class="container postsIndex">

<ul class="row">
    <% posts.forEach((post) => { %>
    <div class="col-lg-4 col-md-6 col-sm-12">
      <div class="container">
          <a href="/posts/<%= post.id %>"><img src="<%= post.imageSRC %>"></a>
          <h3><%= post.caption %></h3>
          </a>
          <h4>Created By: <%= post.createdBy.username %></h4>
          <% }) %>
      </div>
    </div>
</ul>

<a href="/posts/new"><button class="btn">New</button></a>


</div>
